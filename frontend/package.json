  "description": "GSU Panther Chatbot Frontend - Interactive Demo",
  "private": true,
  "dependencies": {
    "babel-loader": "8.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "schema-utils": "3.3.0"
  },
  "scripts": {
    "prestart": "rm -rf node_modules/fork-ts-checker-webpack-plugin/node_modules/ajv* node_modules/fork-ts-checker-webpack-plugin/node_modules/schema-utils node_modules/schema-utils/node_modules/ajv-keywords 2>/dev/null || true",
